{
  "name": "ionic-tooltips",
  "version": "2.0.1",
  "description": "Tooltips module for Ionic apps",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prebuild": "rimraf -rf aot dist ",
    "build": "ngc",
    "copy-dist": "gulp copy-dist",
    "prepare": "npm install rimraf && npm run build && npm run copy-dist",
    "postversion": "npm run changelog",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "postchangelog": "git commit -am \"chore(): update changelog\"",
    "shipit": "npm run lint && npm run build && npm publish",
    "lint": "tslint \"src/**/*.ts\"",
    "test:watch": "karma start ./test-config/karma.conf.js",
    "test": "karma start ./test-config/karma.conf.js --single-run"
  },
  "files": [
    "dist"
  ],
  "author": "Ibby Hadeed <ibby@zyra.ca>",
  "license": "MIT",
  "devDependencies": {
    "@angular/animations": "^5.2.11",
    "@angular/cli": "^1.7.4",
    "@angular/common": "^5.2.11",
    "@angular/compiler": "^5.2.11",
    "@angular/compiler-cli": "^5.2.11",
    "@angular/core": "^5.2.11",
    "@angular/forms": "^5.2.11",
    "@angular/http": "^5.2.11",
    "@angular/platform-browser": "^5.2.11",
    "@angular/platform-browser-dynamic": "^5.2.11",
    "@angular/platform-server": "^5.2.11",
    "@types/jasmine": "^2.8.8",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "^4.4.2",
    "conventional-changelog-cli": "^2.0.1",
    "gulp": "^3.9.1",
    "ionic-angular": "^3.9.2",
    "ionicons": "3.0.0",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jasmine": "^3.1.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "rimraf": "^2.6.2",
    "rxjs": "^5.5.11",
    "ts-loader": "^3.5.0",
    "tslint": "^5.11.0",
    "typescript": "2.4.2",
    "zone.js": "^0.8.26"
  },
  "peerDependencies": {
    "ionic-angular": "^3.0.0",
    "@angular/animations": "*"
  }
